{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/uploadimage",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "uploadimage"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/uploadimage/releases/uploadimage_4.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/uploadwidget",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "uploadwidget"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/uploadwidget/releases/uploadwidget_4.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/filetools",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "filetools"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/filetools/releases/filetools_4.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/notification",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "notification"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/notification/releases/notification_4.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/notificationaggregator",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "notificationaggregator"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/notificationaggregator/releases/notificationaggregator_4.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dropzone/dropzone",
                "version": "5.9",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/dropzone/dropzone.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "ckeditor-plugin/filetools": "^4.8",
        "ckeditor-plugin/notification": "^4.8",
        "ckeditor-plugin/notificationaggregator": "^4.8",
        "ckeditor-plugin/uploadimage": "^4.8",
        "ckeditor-plugin/uploadwidget": "^4.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "dropzone/dropzone": "^5.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/allowed_formats": "^1.5",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_content_permissions": "^1.10",
        "drupal/block_content_template": "^1.4",
        "drupal/ckeditor_acr": "^1.0",
        "drupal/ckeditor_codemirror": "^2.3",
        "drupal/ckeditor_no_autoparagraph": "^1.1",
        "drupal/composite_reference": "^1.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.8",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "~9.3.22",
        "drupal/core-project-message": "~9.3.22",
        "drupal/core-recommended": "~9.3.22",
        "drupal/crop": "^2.2",
        "drupal/ctools": "^3.14",
        "drupal/devel": "^4.1",
        "drupal/devel_php": "^1.5",
        "drupal/draggableviews": "^2.0",
        "drupal/dropzonejs": "^2.6",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/emptyparagraphkiller": "^1.0@alpha",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "^1.8",
        "drupal/field_group": "^3.2",
        "drupal/field_permissions": "^1.1",
        "drupal/file_delete": "^1.0",
        "drupal/google_analytics": "^3.1",
        "drupal/image_style_warmer": "^1.1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/jquery_ui_datepicker": "^1.2",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/layout_options": "^1.0",
        "drupal/layout_paragraphs": "^1.0@beta",
        "drupal/linkit": "^6.0@beta",
        "drupal/media_library_form_element": "^2.0",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.19",
        "drupal/override_node_options": "^2.6",
        "drupal/page_manager": "4.x-dev@dev",
        "drupal/paragraphs": "^1.14",
        "drupal/paragraphs_edit": "^2.0@beta",
        "drupal/paragraphs_features": "^1.16",
        "drupal/pathauto": "^1.9",
        "drupal/permissions_filter": "^1.2",
        "drupal/rebuild_cache_access": "^1.7",
        "drupal/redirect": "^1.7",
        "drupal/robots_noindex_nofollow": "^1.1",
        "drupal/schema_metatag": "^2.3",
        "drupal/search_api": "^1.23",
        "drupal/search_api_solr": "^4.2",
        "drupal/select2": "^1.13",
        "drupal/simple_sitemap": "^3.10",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/structure_sync": "^2.0",
        "drupal/subpathauto": "^1.2",
        "drupal/svg_image": "^1.16",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/token": "^1.10",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_tweak": "^3.1",
        "drupal/view_unpublished": "^1.0",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_bulk_operations": "^3.12",
        "drush/drush": "^10.5",
        "guzzlehttp/guzzle": "~6.5.8",
        "guzzlehttp/psr7": "~1.9.0",
        "laminas/laminas-diactoros": "~2.11.1",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "twig/twig": "~2.15.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "patches-file": "composer.patches.json",
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/ckeditor/plugins/{$name}": [
                "vendor:ckeditor-plugin"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/ckeditor_codemirror/composer.libraries.json"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
